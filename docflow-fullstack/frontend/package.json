{
  "name": "docflow-react-node",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test --filter='./apps/**'"
  },
  "devDependencies": {
    "turbo": "^2.1.1"
  },
  "dependencies": {
    
    "react-router-dom": "6.30.1"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "react-router-dom": "6.30.1",
      "react-router": "6.30.1"
    }
  }
}
